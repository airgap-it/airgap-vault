        "URL_SCHEME": "airgap-vault",
        "DEEPLINK_HOST": "vault.airgap.it",
        "DEEPLINK_SCHEME": "https",
        "ANDROID_PATH_PREFIX": "/",
        "ANDROID_2_PATH_PREFIX": "/",
