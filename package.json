  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ionic_copy": "./copy.config.js",
    "ionic_uglifyjs": "./uglifyjs.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "build": {
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
